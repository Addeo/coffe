{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.json",
    "assets": ["**/*.json"],
    "watchAssets": true,
    "outDir": "dist"
  },
  "entryFile": "src/main",
  "generateOptions": {
    "spec": false
  }
}

