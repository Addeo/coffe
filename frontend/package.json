{
  "name": "coffee-admin-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host 0.0.0.0 --port 4202",
    "start": "ng serve --host 0.0.0.0 --port 4202",
    "build": "ng build --configuration production",
    "deploy": "HUSKY=0 ng build --configuration production --base-href /coffe-deploy/ && gh-pages -d dist/coffee-admin --repo https://github.com/Addeo/coffe-deploy.git --branch gh-pages --no-history",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"**/*.{ts,js,json,md,html,scss,css}\"",
    "lintFormat": "npm run lint || true",
    "ionic:serve": "ionic serve",
    "ionic:build": "ionic build",
    "capacitor:build": "cap build",
    "capacitor:sync": "cap sync",
    "build:android": "ng build --configuration production && cap sync android",
    "build:ios": "ng build --configuration production && cap sync ios",
    "open:android": "cap open android",
    "open:ios": "cap open ios",
    "capacitor:add:android": "cap add android",
    "capacitor:add:ios": "cap add ios",
    "capacitor:copy": "cap copy",
    "mobile:build": "npm run build:android && npm run build:ios"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.0",
    "@capacitor/android": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@ionic/angular": "^7.0.0",
    "@ionic/storage-angular": "^4.0.0",
    "coffee-shared": "file:../shared",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^20.2.0",
    "@angular-eslint/eslint-plugin": "^20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/template-parser": "^20.2.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@ionic/angular-toolkit": "^10.0.0",
    "@ionic/cli": "^7.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "gh-pages": "^6.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "typescript": "~5.2.0"
  }
}
